UnlitGeneric
{
	$basetexture "models/player/items/all_class/all_class_oculus_screen"

	$additive 1
	$nocull 1

	Proxies
	{
		TextureScroll
		{
			texturescrollvar "$basetexturetransform"
			texturescrollrate .05
			texturescrollangle 90
		}

		Sine
		{
			Sineperiod 2
			SineMax 1.5
			SineMin .5
			resultVar "$color"
		}
	}
}