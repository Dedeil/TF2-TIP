VertexLitGeneric
{
	$basetexture "models/buildables/dispenser/dispenser_blue"

	$lightwarptexture "models/buildables/sentry1/sentry1_lightwarp"

	$phong 1
	$phongfresnelranges "[0.5 1.5 10]"
	$phongexponent 25
	$phongboost 2
	$basemapalphaphongmask 1

	// Rim lighting parameters
	$rimlight 1				// To enable rim lighting (requires phong)
	$rimlightexponent 4		// Exponent for phong component of rim lighting	
	$rimlightboost 0.5		// Boost for ambient cube component of rim lighting
}