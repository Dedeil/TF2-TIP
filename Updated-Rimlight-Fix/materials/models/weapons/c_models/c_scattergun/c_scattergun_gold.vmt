VertexLitGeneric
{
	$basetexture "models/weapons/c_models/c_scattergun/c_scattergun_gold"

	$lightwarptexture "models/lightwarps/weapon_lightwarp"

	$envmap "cubemaps/cubemap_gold001"
	$envmaptint "[2.50 2.50 1.15]"

	$phong 1
	$phongfresnelranges "[0.5 0.5 3]"
	$halflambert 1
	$phongexponent 90
	$phongboost 10
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$cloakpassenabled 1

	$glowcolor 1

	$sheenpassenabled 1
	$sheenindex 0
	$sheenmap "cubemaps/cubemap_sheen001"
	$sheenmapmask "effects/animatedsheen/animatedsheen0"
	$sheenmapmaskframe 0
	$sheenmaptint "[ 1 1 1 ]"

	$yellow 0

	Proxies
	{
		AnimatedWeaponSheen
		{
			animatedtexturevar "$sheenmapmask"
			animatedtextureframenumvar "$sheenmapmaskframe"
			animatedtextureframerate 40
		}

		invis
		{
		}

		ModelGlowColor
		{
			resultVar "$glowcolor"
		}

		Equals
		{
			srcVar1 "$glowcolor"
			resultVar "$selfillumtint"
		}

		Equals
		{
			srcVar1 "$glowcolor"
			resultVar "$color2"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Multiply
		{
			srcVar1 "$color2"
			srcVar2 "$yellow"
			resultVar "$color2"
		}
	}
}
