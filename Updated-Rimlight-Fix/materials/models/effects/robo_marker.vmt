UnlitTwoTexture
{
	$basetexture "models/effects/robo_marker"
	$texture2 "models/effects/cappoint_beam_lines_b"

	$additive 1

	Proxies
	{
		TextureScroll
		{
			texturescrollvar "$texture2transform"
			texturescrollrate .1
			texturescrollangle 90
		}

		Sine
		{
			Sineperiod 1
			SineMax 1
			SineMin .8
			resultVar "$color"
		}
	}
}