VertexLitGeneric
{
	$basetexture "models/effects/smiley_emoticon"
	$bumpmap "models/effects/flat_normal"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.3 0.5 0.6]"
	$halflambert 1
	$phongexponent 12

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$yellow 0

	Proxies
	{
		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		BurnLevel
		{
			resultVar "$detailblendfactor"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}