VertexLitGeneric
{
	$basetexture "models/player/items/all_class/all_class_ring"
	$bumpmap "models/effects/flat_normal"

	$lightwarptexture "models/lightwarps/weapon_lightwarp"

	$envmap "env_cubemap"
	$basealphaenvmapmask 1
	$envmaptint "[0.25 0.25 0.25]"

	$phong 1
	$phongfresnelranges "[0.25 1 4]"
	$phongexponent 55
	$phongboost 4
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 15
	$rimlightboost 0.5

	$selfillum 1

	$cloakpassenabled 1

	$yellow 0

	<DX90
	{
		$basemapalphaphongmask "0"
		$normalmapalphaenvmapmask "1"
	}

	Proxies
	{
		weapon_invis
		{
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}