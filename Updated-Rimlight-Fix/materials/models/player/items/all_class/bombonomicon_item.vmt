VertexLitGeneric
{
	$basetexture "models/player/items/all_class/bombonomicon_item"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.25 1 4]"
	$phongexponent 25
	$phongboost 0
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$selfillum 1

	$cloakpassenabled 1

	$yellow 0

	<DX90
	{
		$selfillum "0"
	}

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}