VertexLitGeneric
{
	$basetexture "models/player/items/all_class/brutal_eddiehair"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.3 1 8]"
	$phongexponent 20
	$phongboost 0.05

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$blendtintcoloroverbase 1

	$cloakpassenabled 1

	$colortint_base "{26 26 26}"
	$colortint_tmp "[0 0 0]"

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		ItemTintColor
		{
			resultVar "$colortint_tmp"
		}

		SelectFirstIfNonZero
		{
			srcVar1 "$colortint_tmp"
			srcVar2 "$colortint_base"
			resultVar "$color2"
		}
	}
}