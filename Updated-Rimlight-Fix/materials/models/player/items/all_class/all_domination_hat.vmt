VertexLitGeneric
{
	$basetexture "models/player/items/all_class/all_domination_hat"
	$bumpmap "models/effects/flat_normal"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.25 1 4]"
	$phongexponent 50
	$phongboost 0.5
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$blendtintbybasealpha 1

	$blendtintcoloroverbase 0

	$cloakpassenabled 1

	$colortint_base "{56 48 48}"
	$colortint_tmp "[0 0 0]"

	$yellow 0

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		ItemTintColor
		{
			resultVar "$colortint_tmp"
		}

		SelectFirstIfNonZero
		{
			srcVar1 "$colortint_tmp"
			srcVar2 "$colortint_base"
			resultVar "$color2"
		}

		Multiply
		{
			srcVar1 $color2
			srcVar2 $yellow
			resultvar $color2
		}
	}
}