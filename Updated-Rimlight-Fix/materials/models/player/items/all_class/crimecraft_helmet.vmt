VertexLitGeneric
{
	$basetexture "models/player/items/all_class/crimecraft_helmet"
	$bumpmap "models/effects/flat_normal"

	$lightwarptexture "models/lightwarps/weapon_lightwarp"

	$envmap "env_cubemap"
	$envmaptint "[0.05 0.05 0.05]"

	$phong 1
	$phongfresnelranges "[0.25 1 4]"
	$phongexponent 50
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$blendtintbybasealpha 1
	$blendtintcoloroverbase 0

	$cloakpassenabled 1

	$colortint_base "{169 62 62}"
	$colortint_tmp "[0 0 0]"

	Proxies
	{
		weapon_invis
		{
		}

		ItemTintColor
		{
			resultVar "$colortint_tmp"
		}

		SelectFirstIfNonZero
		{
			srcVar1 "$colortint_tmp"
			srcVar2 "$colortint_base"
			resultVar "$color2"
		}
	}
}