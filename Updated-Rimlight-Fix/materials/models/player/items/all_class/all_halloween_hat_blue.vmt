VertexLitGeneric
{
	$basetexture "models/player/items/all_class/all_halloween_hat_blue"
	$bumpmap "models/effects/flat_normal"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$nocull 1

	$phong 1
	$phongfresnelranges "[0.25 1 4]"
	$phongexponent 1
	$phongboost 0.1
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$cloakpassenabled 1

	$yellow 0

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}