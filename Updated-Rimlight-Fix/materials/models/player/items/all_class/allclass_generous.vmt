VertexLitGeneric
{
	$basetexture "models/player/items/all_class/allclass_generous"
	$bumpmap "models/player/items/all_class/allclass_generous_normal"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.50 0.50 0.50]"

	$phong 1
	$phongfresnelranges "[0.5 1.5 10]"
	$phongexponent 50
	$phongboost 3

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$blendtintbybasealpha 1
	$blendtintcoloroverbase 0

	$cloakpassenabled 1

	$colortint_base "{184 56 59}"
	$colortint_tmp "[0 0 0]"

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		ItemTintColor
		{
			resultVar "$colortint_tmp"
		}

		SelectFirstIfNonZero
		{
			srcVar1 "$colortint_tmp"
			srcVar2 "$colortint_base"
			resultVar "$color2"
		}
	}
}