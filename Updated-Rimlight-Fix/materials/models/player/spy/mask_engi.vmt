VertexLitGeneric
{
	$basetexture "models/player/spy/mask_engi"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.7 15 20]"
	$halflambert 1
	$phongexponent 10
	$phongboost 0.2
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$cloakpassenabled 1

	$yellow 0

	Proxies
	{
		spy_invis
		{
		}

		invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		BurnLevel
		{
			resultVar "$detailblendfactor"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}