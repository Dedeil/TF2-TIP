VertexLitGeneric
{
	$basetexture "models/bots/engineer/engineer_bot_head_blue"
	$surfaceprop Metal

	$lightwarptexture "models/lightwarps/robot_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongexponenttexture "models/bots/heavy/heavy_bot_head_exp"
	$phongfresnelranges "[0.6 0.9 1]"
	$phongboost 2
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5
	$rimmask 1

	$selfillum 1

	$cloakpassenabled 1

	$yellow 0

	<DX90
	{
		$selfillum "0"
	}

	Proxies
	{
		spy_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		BurnLevel
		{
			resultVar "$detailblendfactor"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}