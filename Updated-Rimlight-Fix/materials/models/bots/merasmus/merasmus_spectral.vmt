VertexLitGeneric
{
	$basetexture "models/bots/merasmus/merasmus_spectral"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0.01

	$nocull 1

	$phong 1
	$phongfresnelranges "[0.3 1 8]"
	$phongtint "[0 1 0.30]"
	$phongexponent 20
	$phongboost 0.3

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$selfillum 1
	$selfillumfresnel 1
	$selfillumfresnelminmaxexp "[1 1 1]"
	$selfillumtint "[2 3 2]"

	$cloakpassenabled 0

	$yellow 0

	<DX90
	{
		$selfillum "0"
	}

	Proxies
	{
		spy_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		BurnLevel
		{
			resultVar "$detailblendfactor"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}