VertexLitGeneric
{
	$basetexture "models/bots/skeleton/hwn_skeleton_green"

	$lightwarptexture "models/player/pyro/pyro_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 1
	$detailscale 0.5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.25 0.5 1]"
	$phongexponent 15
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5

	$selfillum 1
	$selfillumfresnel 1
	$selfillumfresnelminmaxexp "[.05 0 25]"
	$selfillumtint "[142 220 41]"

	$nodecal 1

	$cloakpassenabled 1

	$yellow 0

	<DX90
	{
		$selfillum "0"
	}

	Proxies
	{
		weapon_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}

		Sine
		{
			sineperiod 2
			sinemin .00125
			sinemax .1
			resultVar "$selfillumfresnelminmaxexp[1]"
		}

		Sine
		{
			sineperiod 2
			sinemin 0
			sinemax 55
			resultVar "$selfillumfresnelminmaxexp[3]"
		}

		Sine
		{
			sineperiod 1
			sinemin 25
			sinemax 5
			resultVar "$rimlightboost"
		}
	}
}