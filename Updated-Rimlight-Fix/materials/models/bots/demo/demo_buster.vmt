VertexLitGeneric
{
	$basetexture "models/bots/demo/demo_buster"
	$bumpmap "models/bots/demo/demo_buster_normal"
	$surfaceprop Metal

	$lightwarptexture "models/lightwarps/robot_lightwarp"

	$detail "effects/tiledfire/firelayeredslowtiled512"
	$detailblendmode 6
	$detailscale 5
	$detailblendfactor 0

	$phong 1
	$phongfresnelranges "[0.5 0.7 1]"
	$phongexponent 200
	$phongboost 4
	$basemapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.5
	$rimmask 1

	$selfillum 1

	$cloakpassenabled 1

	$yellow 0

	Proxies
	{
		spy_invis
		{
		}

		AnimatedTexture
		{
			animatedtexturevar "$detail"
			animatedtextureframenumvar "$detailframe"
			animatedtextureframerate 30
		}

		BurnLevel
		{
			resultVar "$detailblendfactor"
		}

		YellowLevel
		{
			resultVar "$yellow"
		}

		Equals
		{
			srcVar1 "$yellow"
			resultVar "$color2"
		}
	}
}